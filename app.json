{
  "expo": {
    "name": "Cleaner Log",
    "slug": "AwesomeProject",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/cleanerqr.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mtcsynergy.cleanerlog",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access camera and take photos for your submission task."
      }
    },
    "android": {
      "package": "myqrproject.sampleapps",
      "adaptiveIcon": {
        "foregroundImage": "./assets/cleanerlogo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8084f317-c255-4b9f-9c86-2bce25dc7c1a"
      }
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera and take photos for your submission task."
        }
      ],
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera and scan the QR code."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos and upload for your submission task.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos after successfully created services area.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ]
  }
}
